<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="Dalamud.Plugin.Bootstrap.targets" />
  <PropertyGroup>
    <Version>0.0.0.1</Version>
    <Description>A sample plugin.</Description>
    <PackageProjectUrl>https://github.com/goatcorp/SamplePlugin</PackageProjectUrl>
    <PackageLicenseExpression>AGPL-3.0-or-later</PackageLicenseExpression>
    <IsPackable>false</IsPackable>
    <TargetFramework>net9.0-windows7.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="..\Data\background.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Visible>false</Visible>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <Reference Include="ECommons">
      <HintPath>..\..\..\Dalamud\Main\Dev\AEAssistCNVersion\AEAssist\ECommons.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="ECommons" Version="3.0.0.7" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="ImGuiNET">
      <HintPath>$(AppData)\XIVLauncherCN\addon\Hooks\dev\ImGuiNET.dll</HintPath>
    </Reference>
    <Reference Include="Dalamud">
      <HintPath>$(AppData)\XIVLauncherCN\addon\Hooks\dev\Dalamud.dll</HintPath>
    </Reference>
  </ItemGroup>
  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
  </Target>
</Project>